  RowBox[{"IndependentFormFactors", "[", 
   RowBox[{"formfactors_List", ",", "mattercontent_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"localff", "=", "formfactors"}], ",", "couples", ",", "length", 
      ",", 
      RowBox[{"stop", "=", "True"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", "\[IndentingNewLine]", 
      RowBox[{"stop", ",", "\[IndentingNewLine]", 
