        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "localff", "]"}], "\[Equal]", "1"}], ",", 
          RowBox[{"Return", "[", "localff", "]"}]}], "]"}], ";", " ", 
        RowBox[{"(*", 
         RowBox[{
         "one", " ", "single", " ", "form", " ", "factor", " ", "cannot", " ",
           "be", " ", "repeated"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"couples", "=", 
